if ... == nil then
  print(\Please include a file.\)
else
  inputfile = fs.open(\/scripts/\ .. ..., \r\)
  local out = http.post(\http://machoke-54.dynamic.rpi.edu/loadFile.php\, \name=\ .. ... .. \&data=\.. string.gsub(inputfile.readAll(), \&\, \&\))
 
  write(out.readAll())
   
  inputfile.close()
  out.close()
end