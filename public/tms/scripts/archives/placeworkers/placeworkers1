for index = 1, 2 do
  turtle.forward()
  turtle.turnRight()
  --turtle.select(1)
  --turtle.place()
  --turtle.up()
  turtle.select(2)
  turtle.place()
  turtle.select(3)
  turtle.drop()
  turtle.down()
  peripheral.call("front", "reboot")
  sleep(2)
  turtle.up()
  turtle.suck()
  turtle.select(2)
  turtle.dig()
  --turtle.down()
  turtle.turnLeft()
end