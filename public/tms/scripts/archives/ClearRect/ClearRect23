maxx = 2
maxy = 2
maxz = 1

currentx = 0
currenty = 1
currentz = 1

dir = 0

repeat --Z dir
    print("Z")
    repeat --Y dir
        print("Y")
        repeat --X dir
			currentx = currentx + 1
            --Move in X dir.
			turtle.dig()
			turtle.forward()
		until (currentx >= maxx)

        --If this isn't the last block in our level.
        if (currenty < maxy) then
            --Move in Y dir.
            if (dir == 0) then
                turtle.turnLeft()
                turtle.dig()
                turtle.forward()
                turtle.turnLeft()

            else
                turtle.turnRight()
                turtle.dig()
                turtle.forward()
                turtle.turnRight()
            end
        end

        --Prepare for next column.
        dir = (dir+1)%2
        currentx = 1;
        currenty = currenty + 1

	until (currenty > maxz)

    --Move in Z dir.
    if (currentz < maxz) then
        turtle.digUp()
        turtle.up()
        turtle.turnLeft()
        turtle.turnLeft()
        currenty = 1;
        currentz = currentz + 1
    end
until (currentz >= maxz)